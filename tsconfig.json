{
  "compileOnSave": true,
  "extends": "./tsconfig.strictest.json",
  "compilerOptions": {
    "incremental": true,
    "target": "ESNext",
    "lib": [
      "DOM",
      "DOM.Iterable",
      "ESNext"
    ],
    "noEmit": true,
    "moduleResolution": "node",
    "allowUmdGlobalAccess": true
  },
  "include": ["bin/*"]
}
